{
	"typescript": {
		"lineWidth": 80,
		"semiColons": "always",
		"preferSingleLine": true,
		"memberExpression.linePerExpression": true,
		"binaryExpression.linePerExpression": true
	},
	"markdown": {},
	"includes": [
		"src/**/*.ts",
		"test/**/*.ts",
		"readme.md",
		"spec.md"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.95.15.wasm",
		"https://plugins.dprint.dev/markdown-0.20.0.wasm",
	]
}
